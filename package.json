{
  "name": "to-do",
  "homepage": "https://ValeriFila.github.io/to-do",
  "imports": {
    "@/*": {
      "default": "./src/*",
      "production": "./build/*"
    }
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/config/jest/mocks/styleMock.js",
      "\\.(svg|png)$": "<rootDir>/config/jest/mocks/fileMock.js",
      "^@[/](.+)": "<rootDir>/src/$1"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
  "version": "1.0.0",
  "dependencies": {
    "@babel/core": "^7.24.7",
    "@reduxjs/toolkit": "^2.2.6",
    "i18next": "^23.11.5",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-http-backend": "^2.5.2",
    "moment": "^2.30.1",
    "react": "^18.2.0",
    "react-big-calendar": "^1.13.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.1.2",
    "react-icons": "^5.2.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.22.1"
  },
  "scripts": {
    "start:dev": "webpack serve --env mode=development",
    "start:prod": "webpack serve",
    "build:dev": "webpack --env mode=development",
    "build:prod": "webpack --env mode=production",
    "lint": "npx eslint \"**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "npx eslint --fix \"**/*.{js,jsx,ts,tsx}\"",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint --fix \"**/*.scss\"",
    "unit": "jest -c ./config/jest/jest.config.ts",
    "predeploy": "npm run build:dev",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@eslint/js": "^9.5.0",
    "@jest/globals": "^29.7.0",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/dom": "^10.1.0",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@types/react": "^18.3.3",
    "@types/react-big-calendar": "^1.8.9",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/webpack": "^5.28.5",
    "@types/webpack-dev-server": "^4.7.2",
    "axios": "^1.7.2",
    "babel-jest": "^29.7.0",
    "babel-plugin-i18next-extract": "^0.10.0",
    "css-loader": "^7.1.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-webpack-plugin": "^4.2.0",
    "file-loader": "^6.2.0",
    "gh-pages": "^6.1.1",
    "globals": "^15.6.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mini-css-extract-plugin": "^2.8.1",
    "sass": "^1.74.1",
    "sass-loader": "^14.1.1",
    "style-loader": "^4.0.0",
    "styled-components": "^6.1.11",
    "stylelint": "^16.6.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "ts-jest": "^29.1.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.1.6",
    "typescript-eslint": "^7.13.1",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "overrides": {
    "eslint": "^9.5.0"
  }
}
